{
    "name": "surfoo/geocaching-api-demo",
    "description": "Geocaching PHP SDK Demo",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Surfoo",
            "email": "surfooo@gmail.com"
        }
    ],
    "require": {
        "twig/twig": "^2.4",
        "surfoo/geocaching-php-sdk": "^3.7",
        "twbs/bootstrap": "^4.1",
        "components/jquery": "^3.3",
        "symfony/debug": "^4.1"
    },
    "scripts": {
        "assets": [
            "cp -r vendor/twbs/bootstrap/dist/* web/assets/bootstrap/",
            "cp -r vendor/components/jquery/* web/assets/jquery/"
        ],
        "post-install-cmd": [
            "cp -n app/config.dist.php app/config.php",
            "@assets"
        ],
        "post-update-cmd": [
            "@assets"
        ]
    }
}
